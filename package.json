{
  "name": "@mosaia/mosaia-node-sdk",
  "version": "0.0.28",
  "description": "Mosaia's TypeScript SDK for constructing 3rd party app integrations on the Mosaia platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:runner": "tsx src/__tests__/run-tests.ts",
    "test:all": "npm run test:coverage && npm run test:runner",
    "test:circular": "tsx src/__tests__/circular-dependency.test.ts",
    "test:circular:js": "node src/__tests__/circular-dependency.test.js",
    "test:circular:simple": "tsx src/__tests__/circular-dependency-simple.test.ts",
    "docs": "typedoc",
    "docs:build": "npm run build && npm run docs",
    "docs:watch": "typedoc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mosaia-development/mosaia-node-sdk.git"
  },
  "author": "Mosaia",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mosaia-development/mosaia-node-sdk/issues"
  },
  "homepage": "https://github.com/mosaia-development/mosaia-node-sdk#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.9.1",
    "dotenv": "^17.2.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.20.3",
    "typescript": "^5.6.3",
    "typedoc": "^0.27.1"
  }
}
